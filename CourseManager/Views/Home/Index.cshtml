@model CourseManager.Models.Websitelnfo

@using CourseManager.Models
@{
    ViewBag.Title = "主页";

    var sitIfoFromViewBag = (Websitelnfo)ViewBag.SiteInfo;

    var sitIfoFromViewData = (Websitelnfo)ViewData["SiteInfo"];
}

<div  class="jumbotron">
      <h1>@Model.SiteName</h1>
      <p>@Model.CopyRight</p>
</div>

<div  class="jumbotron">
      <h1>@sitIfoFromViewBag.SiteName</h1>
      <p>@sitIfoFromViewBag.CopyRight</p>
</div>

<div  class="jumbotron">
      <h1>@sitIfoFromViewData.SiteName</h1>
      <p>@sitIfoFromViewData.CopyRight</p>
</div>